    			    font_size:dp(9)
    				text:f'{" "*int(dp(7))} Registration{" "*int(dp(7))}'    
                    bold:True
    				pos_hint:{'center_x':0.5,'center_y':0.16}  				
                    elevation:10
                  
                    on_release:root.loading_func()
               
    MDBoxLayout:
        orientation:'vertical'
        MDToolbar:
            title:'Registration'
        	left_action_items:[["keyboard-backspace",lambda x: root.screen_back() ]]
        	elevation:9
        Widget:
                            		

                        

