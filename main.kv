ScreenManager:
    transition:CardTransition()
    Login_Screen:    
    Registration_Screen:
    Main_Screen:		
	Account_Screen:
	Upload_Screen:
	Filemanager_Screen:			   	      
    About_Screen:
    Help_Screen:
    Reward_Screen:    	
    Forgot_Screen:
    	
        
                
	       
<Login_Screen>:
    name:'login_screen'
    on_enter:
        root.anim()
    canvas.before:
        Rectangle:
			size:self.size
			pos:self.pos
			source:'App_background.jpg'
	MDCard:
	    id:card
	    canvas:
	        RoundedRectangle:
	            size:self.size
	            pos:self.pos
	            radius:[30]
	            source:'App_background.jpg'
		size_hint:None,None
		size:('300dp','450dp')
		padding:'8dp'
		eevation:8
		focus_behavior:1
		ripple_behavior:1
		border_radius: 30
        radius: [30]
		pos_hint:{"center_x":0.5,"center_y":0.5}
		opacity:1
		MDFloatLayout:
			MDLabel:
			    id:login_title
				text:'LOGIN'
				bold:True
				theme_text_color:'Custom'
				text_color:(0,0,0,1)
				height:self.texture_size[1] 
				valign:'middle'
				font_style:'H4'
				halign:'center'
				pos_hint:{'center_x':0.5,'y':0.43}
			MDLabel:
				text:'Email Address :'
			    valign:'middle' 
				halign:'left'
		    	theme_text_color:'Custom'
				text_color:0.56,0,1,1
				height:self.texture_size[1] 
				pos_hint:{'x':0.0,'y':0.33}
				padding_x:dp(7)
		
		
			MDTextField:
				id:email
				hint_text:'please enter your email id'
		        required:True
			    current_hint_text_color:0,0,0,1
                color_mode:'custom'
                line_color_focus:1,0,0.6,1
				mode:'line'
				icon_right:'account'				
				line_anim:1			               
				pos_hint:{'x':0.027,'center_y':0.75}		
		
			MDLabel:
				text:'Password:'
			    valign:'middle' 
				halign:'left'
		    	theme_text_color:'Custom'
				text_color:0.56,0,1,1
				height:self.texture_size[1] 			
				padding_x:dp(7)
		        pos_hint:{'x':0.0,'y':0.16}
				
			MDTextField:
				id:pass_word
				hint_text:'please enter your password:'				    		
				required:True	
				mode:'line'
				line_anim:1			
				password:True
				current_hint_text_color:0,0,0,1
                color_mode:'custom'
                line_color_focus:1,0,0.6,1
				pos_hint:{'x':0.027,'center_y':0.57}	
	
			IconToggleButton:
				id:toggle_btn
				icon:{'normal':'eye-off','down':'eye'}[self.state]						
				on_state:root.func()
				pos_hint:{'x':0.87,'center_y':0.58}
			Button:
				size_hint:(0.4,0.2)				
				background_normal:'Login_btn.png'
				background_down:'Login_btn.png'
				allow_stretch:True
				on_release:root.login_func()
				pos_hint:{'center_x':0.5,'center_y':0.42}
			MDTextButton:
				text:'Forgot Password'
				font_size:dp(18)
				underline:True
				pos_hint:{'center_x':0.5,'center_y':0.3}
				on_release:root.manager.current='forgot_screen'
			MDTextButton:
				text:'Registration Here'
				font_size:dp(18)
				bold:True
				pos_hint:{'center_x':0.5,'center_y':0.2}
				on_release:root.manager.current='registration_screen'
			MDTextButton:
				text:'Skip Login'
				font_size:dp(16)
				underline:True
				pos_hint:{'center_x':0.5,'center_y':0.1}
				on_release:root.skip()
<Main_Screen>:
	name:'main_screen'
		 
                         
	#on_enter:root.image_show()                       			       	
    NavigationLayout:
        ScreenManager:
            Screen:
                MDBoxLayout:
			        id:box_layout
			        orientation:'vertical'
			        
			        MDToolbar:			            
    	                title:'Gallery Portal'
    			    	left_action_items:[["menu",lambda x: nav_draw.set_state()]] 
    			    	right_action_items:[['theme-light-dark',lambda x:root.show_theme_picker()],['share-variant',lambda x: root.show_example_grid_bottom_sheet()]]   	                	            	
    			    	elevation:9
                 
                    MDFloatLayout:
					    ScrollView:
					        id:scroll
					        always_overscroll:True
					        on_scroll_start: root.remove_btn()
					        on_scroll_stop: root.add_btn()
					        scroll_to:True			        
					        MDGridLayout:
                        	    id:grid
                                cols:2                                                 
                                  #  row_default_height:(self.width-self.cols*self.spacing[0])/self.cols
                                 #  row_force_default:True
                                adaptive_height: True
                                padding:dp(4),dp(4)
                                spacing:dp(8)
                                    
                        MDFloatingActionButton:
                            id:refresh_btn
                            on_release:root.refresh()
                            icon:'refresh'
                            md_bg_color:app.theme_cls.primary_color
                            pos_hint:{'center_x':0.85,'center_y':0.2}
                            opacity:0
                            
                          
              
                            
                                    
                                                                		                                          			       			   	      
        MDNavigationDrawer:
			id:nav_draw
			BoxLayout:
			    id:nav_boxlayout
				orientation:'vertical'
				spacing:dp(7)
				padding:dp(7)
			    
				
				Image:
					id:nav_draw_img
					source:'Account_img.png'
					
				
				MDLabel:
				    id:nav_name
					text:' '
					size_hint_y:None
					font_style:'H6'
					height: self.texture_size[1]
				MDLabel:
				    id:nav_email
					text:' '
					size_hint_y:None
					font_style:'Subtitle1'
					height: self.texture_size[1]
		        ScrollView:
		            MDList:		 
                    	OneLineIconListItem:
                    	    on_release:root.manager.current='account_screen'
                		    text:'Profile'
                		    elevation:10
                            IconLeftWidget:
                                icon: 'account-edit'
                        OneLineIconListItem:
                            on_release:root.manager.current='upload_screen'        	                           
                		    text:'Upload'
                		    elevation:10
                            IconLeftWidget:
                                icon: 'cloud-upload-outline'                          
                        OneLineIconListItem:
                            on_release:root.manager.current='help_screen'
                		    text: 'Help'
                		    elevation:10
                            IconLeftWidget:
                                icon: 'help-circle-outline'
                        OneLineIconListItem:
                            on_release:
                                root.manager.tranition=SwapTransition()
                                root.manager.current='about_screen'
                                #root.manager.transition.duration = 1                              
                		    text: 'About'
                		    elevation:10
                            IconLeftWidget:
                                icon: 'contacts'
                        OneLineIconListItem:
                        	on_release:root.manager.current='reward_screen'
                            text: 'Rewards'
                            elevation:10
                            IconLeftWidget:
                                icon: 'coin-outline'                        
                        OneLineIconListItem:
                            on_release:app.stop()
                		    text:'Exit'
                		    elevation:10
                            IconLeftWidget:
                                icon: 'exit-to-app'



<Registration_Screen>:
    name:'registration_screen'   
    canvas:
        Color:
    	    rgba:91.0/255.0,188.0/255.0,228.0/255.0,1
        Rectangle:
            size:self.size
        	pos:self.pos
    
    MDFloatLayout:                     
        MDCard:
            size_hint:None,None
            size:('300dp','450dp')
            padding:'8dp'
            elevation:8
