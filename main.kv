<Upload_Screen>:
    name:'upload_screen' 
    canvas:
        Color:
            rgba:0.7,0.5,0.9,1
        Rectangle:
            size:self.size
            pos:self.pos
        Color: 
            rgba: 0.5,0.3,1,1
        Rectangle: 
            pos: 0,260
            size: 750,300
        Color: 
            rgba: 0.7,0.5,0.9,1
        Rectangle: 
            pos: 30,288
            size: 650,230
                                                
    Button:
        id:upload_btn
        background_normal:'upload img.png'
        background_down:'upload img.png'
        
        pos:50,600
        size_hint:0.87,0.43
    Label:
        id:label
        text:'No file chosen'
        font_size:dp(20)
        pos:-20,-180
    MDFillRoundFlatIconButton:
        text:'Choose file'
        icon:'file-cloud'
        pos_hint:{'x':0.28,'y':0.25}
        on_release:root.manager.current='filemanager_screen'
    MDSpinner:
        id:loadingbar
        size_hint: None, None
        size: dp(56), dp(56)
        pos_hint: {'center_x': .5, 'center_y': .5}       
        palette:[[0.28627450980392155, 0.8431372549019608, 0.596078431372549, 1],[0.3568627450980392, 0.3215686274509804, 0.8666666666666667, 1],[0.8862745098039215, 0.36470588235294116, 0.592156862745098, 1],[0.8784313725490196, 0.9058823529411765, 0.40784313725490196, 1]]          
        active:False
    
    MDRaisedButton:
        id:upload_submitbtn
        text:f'{" "*int(dp(12))} Upload{" "*int(dp(14))}'            
        font_size:dp(8)
        elevation_normal:10
        
        on_release:root.processing_func()
        pos_hint:{'x':0.1,'y':0.1}
    
    MDBoxLayout:
        orientation:'vertical'
        MDToolbar:
            title:'Firebase Upload'
        	left_action_items:[["keyboard-backspace",lambda x: root.screen_back() ]]             
        	elevation:9
        Widget:
        
  
       
<Filemanager_Screen>:
    name:'filemanager_screen'
